  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.41.2",
    "@types/node": "^24.9.2",
    "allure-commandline": "^2.34.1",
    "cypress": "^15.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
