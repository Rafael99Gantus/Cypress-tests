{
  "name": "18_cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cypress": "cypress open",
    "test": "cypress run",
    "cy:open:sandbox": "cypress open --config \"specPattern=cypress/e2e/sandbox/**/*.cy.ts\"",
    "cy:run:sandbox": "cypress run --spec \"cypress/e2e/sandbox/**/*.cy.ts\"",
    "test:allure:open": "npm run allure:clear && cypress run --env allure=true && allure generate allure-results -o allure-report --clean && allure open allure-report",
    "allure:clear": "rimraf allure-results allure-report || rm -rf allure-results allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rafael99Gantus/Cypress-tests.git"
  },
